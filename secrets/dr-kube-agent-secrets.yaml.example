# dr-kube-agent-secrets.yaml.example
# 실제 사용시: cp dr-kube-agent-secrets.yaml.example dr-kube-agent-secrets.yaml
# 값 입력 후: sops -e dr-kube-agent-secrets.yaml > dr-kube-agent-secrets.enc.yaml
apiVersion: v1
kind: Secret
metadata:
  name: dr-kube-agent-secrets
  namespace: monitoring
type: Opaque
stringData:
  gemini-api-key: "your-gemini-api-key-here"
  github-token: "your-github-token-here"
